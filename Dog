public class Dog {
	private String name;
	private int age;


public Dog(String name, int age) {
	this.name = name;
	this.age = age;
}

public String getName() {
	return name;
}
public int getAge() {
	return age;
}
public void inPersonYears()
{
	age = age*7;
	System.out.println("inPersonYears = "+age);
}
public static void main(String[] args) {
	Dog dog = new Dog("Fido", 5);
	System.out.println("name = "+dog.name);
	System.out.println("age = "+dog.age);
	dog.inPersonYears();
}
}
