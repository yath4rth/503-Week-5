//This import is necessary for your program to run in code validator
import java.lang.reflect.*;

public class Album {
    // Declare  instance variables
	private String title;
	private String artist;
    //Constructor here 
public Album (String title, String artist) {
	this.title = title;
	this.artist = artist;
}
    // Get methods for title and artist
public String getTitle(){
	return title;
}
public String getArtist(){
	return artist;
}    
// Set methods for title and artist
public void setTitle(String title){
	this.title = title;
}
public void setArtist(String artist){
	this.artist = artist;
}


public static void main(String[] args) 
    {
        // Create an Album instance variable
    	Album ba = new Album("Good Tunes", "Great Artist");
    	System.out.println("title = "+ba.getTitle());
    	System.out.println("artist =  "+ba.getArtist());
    	ba.setTitle("Great Tunes");
    	System.out.println("title =  "+ba.getTitle());
    	ba.setArtist("Better Artist");
    	System.out.println("artist =  "+ba.getArtist());
    	
    }
}
